[user]
	name = ikatama
	email = ikatama@example.com
[credential]
    helper = /usr/bin/git/mingw64/libexec/git-core/git-credential-manager-core.exe
[alias]
    graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	alias = config --get-regexp ^alias\\.
	oneline = log --oneline
[color]
	ui = auto

